{
    "name": "ismayil-dev/laravel-doc-kit",
    "description": "A Laravel package for generating OpenAPI documentation and TypeScript SDK.",
    "type": "library",
    "authors": [
        {
            "name": "Ismayil Aliyev",
            "email": "me@ismayil.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "zircote/swagger-php": "^5.0",
        "laravel/framework": "^10.0|^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IsmayilDev\\LaravelDocKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IsmayilDev\\LaravelDocKit\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
