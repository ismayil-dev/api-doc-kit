{
    "name": "ismayil-dev/api-doc-kit",
    "description": "A Laravel package for generating OpenAPI documentation and TypeScript SDK.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "openapi",
        "api-documentation",
        "swagger",
        "api",
        "rest-api",
        "php",
        "typescript",
        "sdk-generator"
    ],
    "homepage": "https://github.com/ismayil-dev/api-doc-kit",
    "authors": [
        {
            "name": "Ismayil Aliyev",
            "email": "me@ismayil.dev"
        }
    ],
    "support": {
        "issues": "https://github.com/ismayil-dev/api-doc-kit/issues",
        "source": "https://github.com/ismayil-dev/api-doc-kit"
    },
    "require": {
        "php": "^8.2",
        "zircote/swagger-php": "^5.0",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "IsmayilDev\\ApiDocKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IsmayilDev\\ApiDocKit\\Tests\\": "tests/",
            "IsmayilDev\\ApiDocKit\\Tests\\Doubles\\DataClasses\\": "tests/doubles/DataClasses/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.7",
        "laravel/pint": "^1.20",
        "nikic/php-parser": "^5.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IsmayilDev\\ApiDocKit\\Providers\\ApiDocKitProServiceProvider"
            ]
        }
    }
}
