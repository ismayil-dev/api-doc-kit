{
    "name": "ismayil-dev/api-doc-kit",
    "description": "A Laravel package for generating OpenAPI documentation and TypeScript SDK.",
    "type": "library",
    "authors": [
        {
            "name": "Ismayil Aliyev",
            "email": "me@ismayil.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "zircote/swagger-php": "^5.0",
        "laravel/framework": "^10.0|^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IsmayilDev\\ApiDocKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IsmayilDev\\ApiDocKit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.7",
        "laravel/pint": "^1.20",
        "nikic/php-parser": "^5.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
